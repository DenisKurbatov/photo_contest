        <div class="container">
          <%= link_to "photo-contest", root_path , id: "logo" %>
          <nav>
            <ul class="nav pull-right">
              
              <% if current_user %>
                
                <li class="dropdown">
                  <a href = "#" class="dropdown-toggle" data-toggle="dropdown">
                    Меню <b class="caret"></b>
                  </a>
                  <ul class="dropdown-menu">
                    <li><%= link_to "Галерея конкурса", photos_path, method: "get"  %></li>
